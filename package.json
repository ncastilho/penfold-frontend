{
  "name": "penfold-site",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "http-proxy-middleware": "^0.19.1",
    "npm-watch": "^0.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
